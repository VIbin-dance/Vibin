<section id="calendar" class="calendar">
    <div class="text-center iframeCal">
        <iframe title="Google Calendar"
            src="https://calendar.google.com/calendar/embed?src=<%= CALENDAR_ID %>&ctz=Asia%2FTokyo" style="border: 0"
            width="100%" height="100%" frameborder="0" scrolling="no"></iframe>
    </div>
    <br>
    <br>
    <div class="container">
        <h4><%= __('calendar.top') %></h3>
            <p><%= __('calendar.sub1') %><%= count %><%= __('calendar.sub2') %>
                <% if (count == 0)  { %>
                <a href="/dashboard/-1?page=1&limit=15">
                    <%= __('calendar.plan') %>
                </a>
                <% } %>
            </p>

            <%- include ('partials/messages') %>
            <div class="row">
                <% for(let i=0; i < count; i++) { %>
                <div class="col-lg-12">
                    <form action="/calendar" method="POST">
                        <h5><a href="/player/<%= id[i] %>"><%= summary[i] %></a></h5>
                        <p><%= dateTime[i] %></p>
                        <input id="idCal" name="idCal" class="form-control" placeholder="Enter The ID"
                            value="<%= typeof idCal[i] != 'undefined' ? idCal[i] : '' %>" hidden />
                        <button class="deleteButton" type="submit">cancel</button>
                    </form>
                    <br>

                </div>
                <% } %>
            </div>
    </div>
</section>