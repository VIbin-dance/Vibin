<header id="header" class="fixed-top">
    <div class="container d-flex align-items-center justify-content-between">
        <h1 class="logo"><a href="/">Vibin'</a></h1>
        <nav class="nav-menu d-none d-lg-block">
            <ul>
                <li><a href="/dashboard/-1?page=1&limit=15">Home</a></li>
                <li class="active"><a href="/calendar">Calendar</a></li>
                <li><a href="/upload">Upload</a></li>
            </ul>
        </nav>
        <nav class="nav-menu d-none d-lg-block">
            <ul>
                <li class="drop-down">
                    <img src="<%= userPhoto %>" height="40" width="40" style="border-radius: 50%;">
                    <ul>
                        <li><a href="/users/profile">Profile</a></li>
                        <li><a href="/logout">Logout</a></li>
                    </ul>
                </li>
            </ul>
        </nav>
    </div>
</header>

<section id="calendar" class="calendar">
    <div class="text-center iframeCal">
        <iframe src="https://calendar.google.com/calendar/embed?src=<%= CALENDAR_ID %>&ctz=Asia%2FTokyo"
            style="border: 0" width="100%" height="100%" frameborder="0" scrolling="no"></iframe>
    </div>
    <br>
    <br>
    <div class="container">
        <h4>Upcoming vibes!</h3>
            <p>We have <%= count %> sessions ahead!
                <% if (count == 0)  { %>
                <a href="/dashboard/-1?page=1&limit=15">
                    Let's plan our sessions!
                </a>
                <% } %>
            </p>

            <%- include ('partials/messages') %>
            <div class="row">
                <% for(let i=0; i < count; i++) { %>
                <div class="col-lg-12">
                    <form action="/calendar" method="POST">
                        <h5><a href="/player/<%= id[i] %>"><%= summary[i] %></a></h5>
                        <p><%= dateTime[i] %></p>
                        <input id="idCal" name="idCal" class="form-control" placeholder="Enter The ID"
                            value="<%= typeof idCal[i] != 'undefined' ? idCal[i] : '' %>" hidden />
                        <button class="deleteButton" type="submit">cancel</button>
                    </form>
                    <br>

                </div>
                <% } %>
            </div>
    </div>
</section>

<div class="container">
    <nav class="fixed-bottom navbar navbar-dark bg-light justify-content-between align-items-center d-flex">
        <ul>
            <li class="active"><a href="/dashboard/-1?page=1&limit=15">Home</a></li>
