<script>
    function listEvent() {
        window.id = [];
        let API_key = '<%= API_key %>'
        let CALENDAR_ID = '<%= CALENDAR_ID %>'
        let Accesstoken = '<%= accessToken %>'
        let time = encodeURIComponent(moment().format());

        const listURL = `https://www.googleapis.com/calendar/v3/calendars/${CALENDAR_ID}/events?orderBy=startTime&q=vibin&singleEvents=true&timeMin=${time}&key=${API_key}`

        fetch(listURL, {
            'headers': { 'Authorization': `Bearer ${Accesstoken}` },
        })
            .then(response => response.json())
            .then(data => {
                if (data.items.length === 0) {
                    document.getElementById("msg").innerHTML = "<span>Looks like you have no vibes planned! Check <a href = '/dashboard?page=1&limit=15'>more dance</a></span>";
                } else {
                    for (i = 0; i < data.items.length; i++) {
                        document.getElementById("summary" + i).innerHTML = `<span><a href='/player/${data.items[i].description}'>${data.items[i].summary}</a></span>`;
                        document.getElementById("time" + i).innerHTML = moment(data.items[i].start.dateTime).format('MMMM Do YYYY, h:mm a');
                    }
                }
            })
    }
</script>