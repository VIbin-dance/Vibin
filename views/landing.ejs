<section id="hero" class="d-flex align-items-center">
  <div class="container">
    <%- include ('partials/messages') %>
      <div class="row">
        <div class="col-lg-6 pt-2 pt-lg-0 order-2 order-lg-1 d-flex flex-column justify-content-center">
          <h1><strong>
              <%= __('landing.hero.top') %>
            </strong></h1>
          <ul>
            <li><i class="ri-check-line"></i>
              <%= __('landing.hero.reason1') %>
            </li>
            <li><i class="ri-check-line"></i>
              <%= __('landing.hero.reason2') %>
            </li>
            <li><i class="ri-check-line"></i>
              <%= __('landing.hero.reason3') %>
            </li>
          </ul>
          <div class="mt-3">
            <a href="/auth/google" class="btn-get-started scrollto">
              <%= __('landing.hero.Get started') %>
            </a>
          </div>
        </div>
        <div class="col-lg-6 order-1 order-lg-2 hero-img">
          <img src="assets/img/image_2.jpg" class="img-fluid" alt="">
        </div>
      </div>
  </div>
</section>

<% if (lesson.docs.length) { %>
  <section id="results" class="results">
    <div class="">
        <h3>
          これからのレッスン&#10024;
        </h3>
        <div class="row">
          <% for(let i=0; i < lesson.docs.length; i++) { %>
            <% if(moment(lesson.docs[i].time).isAfter(moment())) { %>
              <div class="col-lg-3 col-xs-6">
                <div class="box">
                  <a href="/reservation/<%= lesson.docs[i].id %>">
                    <img
                      src="data:image/<%=lesson.docs[i].thumbnail.contentType%>;base64, <%=lesson.docs[i].thumbnail.data.toString('base64')%>"
                      alt="thumbnail">
                  </a>
                  <h4 id="title<%= i %>">
                    <%= lesson.docs[i].title %>
                  </h4>
                  <a class="special" href="/choreographer/<%= lesson.docs[i].choreographerID %>">
                    <%= choreographer[i].username %>
                  </a>
                  <p>
                    <%= lesson.docs[i].price %>
                      <%= __('dashboard.Yen') %> |
                        <%= moment(lesson.docs[i].time).format('MM/DD HH:mm'); %>
                  </p>
                  <p class="mb-0">
                    <%= __(`dashboard.search.level.${lesson.docs[i].level[0]}`) %> |
                      <%= __(`dashboard.search.genre.${lesson.docs[i].genre[0]}`) %> |
                        <%= __(`dashboard.search.purpose.${lesson.docs[i].purpose[0]}`) %> |
                          <%= __(`dashboard.search.mood.${lesson.docs[i].mood[0]}`) %>
                  </p>
                </div>
              </div>
              <% } %>
                <% } %>
        </div>
    </div>
  </section>
  <% } %>

