<body id="myNavbar" data-aos-easing="ease-in-out" data-aos-duration="500" data-aos-delay="0" class="loaded_page">
    <div id="wrapper">
        <div id="content">
            <!-- Stat main -->
            <main data-spy="scroll" data-target="#navbar-example2" data-offset="0">

                <!-- Start banner_about -->
                <section class="pt_banner_inner banner_px_image single_blog type_background">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-7 col-lg-7">
                                <div class="banner_title_inner">
                                    <div class="about_post">
                                        <span class="c_ategory">
                                            <a href="#">
                                                <%= __(`dashboard.search.level.${lesson.level[0]}`) %>
                                            </a>
                                            <a href="#">
                                                <%= __(`dashboard.search.genre.${lesson.genre[0]}`) %>
                                            </a>
                                        </span>
                                        <span class="dot"></span>
                                        <span class="c_ategory">
                                            <%= lesson.price %>円
                                        </span>
                                        <span class="dot"></span>
                                        <time>
                                            <%= moment(lesson.time.start).format('MM/DD HH:mm'); %> ~
                                                <%= lesson.time.end %>
                                        </time>
                                    </div>
                                    <h1 class="margin-my-3 font-s-60 c-dark aos-init aos-animate" style="margin-left: 0;">
                                        <%= lesson.title %>
                                    </h1>
                                    <div class="footer_content">
                                        <div class="item_auther">
                                            <div class="media">
                                                <img src="<%= choreographer.userPhoto %>" alt="">
                                                <div class="media-body my-auto">
                                                    <div class="txt">
                                                        <h4>
                                                            <%= choreographer.username %>
                                                        </h4>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <section>
                                        <form action="/create-checkout-session/<%= params %>" method="POST">
                                            <button type="submit" id="checkout-button"
                                            class="btn btn_lg_primary scale border-0 sweep_letter sweep_top bg-pink c-white rounded-pill mt-5">
                                                <span>
                                                    レッスンを<%= __('reservation.purchase') %>
                                                </span>
                                            </button>
                                        </form>
                                </div>
                            </div>
                            <div class="col-md-5 col-lg-5">
                                <img class="rounded-15 img-fluid" src="<%= lesson.thumbnail %>" alt="">
                            </div>
                        </div>

                    </div>
                </section>
                <!-- End banner_about -->

                <!-- Start content-Sblog -->
                <section class="content-Sblog" data-sticky-container="">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-3">
                                <div class="fixSide_scroll" data-sticky-for="1023" data-margin-top="100" style="">
                                    <div class="share_socail">
                                        <div class="title">共有する</div>

                                        <button class="btn icon" data-toggle="tooltip" data-placement="right" title=""
                                            data-sharer="facebook" data-hashtag="Vibin'"
                                            data-url="https://vibin.tokyo/reservation/<%= lesson._id %>"
                                            data-original-title="Facebook">
                                            <i class="tio facebook"></i>
                                        </button>

                                        <button class="btn icon" data-toggle="tooltip" data-placement="right" title=""
                                            data-sharer="twitter" data-title="<%= lesson.title %> | Vibin'"
                                            data-hashtags="vibin"
                                            data-url="https://vibin.tokyo/reservation/<%= lesson._id %>"
                                            data-original-title="Twitter">
                                            <i class="tio twitter"></i>
                                        </button>

                                        <button class="btn icon" data-toggle="tooltip" data-placement="right" title=""
                                            data-sharer="pinterest"
                                            data-url="https://vibin.tokyo/reservation/<%= lesson._id %>"
                                            data-original-title="Pinterest">
                                            <i class="tio pinterest_circle"></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-8 mt-3">
                                <div class="body_content">
                                    <p class="txt_quotation">
                                        <%= moment(lesson.time.start).format('MM/DD HH:mm'); %> ~
                                            <%= lesson.time.end %>
                                                <br>
                                                <b>
                                                    <%= lesson.price %>円
                                                </b>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
                <!-- End. content-Sblog -->
                <!-- Start section_tag_auther -->
                <section class="section_tag_auther">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-5 offset-lg-3">
                                <div class="box_tags_psot">
                                    <h4>タグ</h4>
                                    <a href="#" class="btn">
                                        <%= __(`dashboard.search.level.${lesson.level[0]}`) %>
                                    </a>
                                    <a href="#" class="btn">
                                        <%= __(`dashboard.search.genre.${lesson.genre[0]}`) %>
                                    </a>
                                </div>
                            </div>
                            <div class="col-lg-9 offset-lg-3">
                                <div class="block_auther_post margin-t-5">
                                    <div class="row">
                                        <div class="col-lg-7">
                                            <div class="media">
                                                <img class="img_person" src="<%= choreographer.userPhoto %>" alt="">
                                                <div class="media-body">
                                                    <div class="txt">
                                                        <h4>
                                                            <%= choreographer.username %>
                                                        </h4>
                                                        <p>
                                                            <%= choreographer.bio %>
                                                        </p>
                                                        <p>

                                                            <a href="<%= choreographer.website %>">
                                                                <%= choreographer.website %>
                                                            </a>
                                                        </p><br>
                                                        <p>
                                                            <%= __('reservation.contact') %>:
                                                                <a href="mailto:<%= choreographer.email %>">
                                                                    <%= choreographer.email %>
                                                                </a>
                                                        </p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4 my-auto ml-auto text-lg-right">
                                            <a href="<%= choreographer.website %>"
                                                class="btn-social color-website border-0  mr-2">
                                                <i class="tio website"></i>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
                <!-- End. section_tag_auther -->

                <!-- Start blog_masonry -->
                <section class="blog_masonry four_column">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="banner_title_inner">
                                    <h1 data-aos="fade-up" data-aos-delay="0" class="aos-init aos-animate">
                                        オススメのレッスン&#10024;
                                    </h1>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <% for(let i=0; i < newLesson.length; i++) { %>
                                <div class="col-md-6 col-lg-4">
                                    <div class="card">
                                        <a href="/reservation/<%= newLesson[i]._id %>" class="link_poet">
                                            <div class="cover_link">
                                                <img class="main_img" src="<%= newLesson[i].thumbnail %>" alt="...">
                                                <div class="auther_post">
                                                    <div class="media">
                                                        <img src="<%= newChoreographer[i].userPhoto %>" alt="">
                                                        <div class="media-body my-auto">
                                                            <div class="txt">
                                                                <h4>
                                                                    <%= newChoreographer[i].username %>
                                                                </h4>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </a>
                                        <div class="card-body">
                                            <div class="about_post">
                                                <span class="c_ategory">
                                                    <a href="#">
                                                        <%= __(`dashboard.search.level.${newLesson[i].level[0]}`) %>
                                                    </a>
                                                    <a href="#">
                                                        <%= __(`dashboard.search.genre.${newLesson[i].genre[0]}`) %>
                                                    </a>
                                                </span>
                                                <span class="dot"></span>
                                                <time>
                                                    <%= moment(newLesson[i].time.start).format('MM/DD HH:mm'); %> ~
                                                        <%= newLesson[i].time.end %>
                                                </time>
                                            </div>
                                            <a href="" class="d-block">
                                                <h5 class="card-title">
                                                    <%= newLesson[i].title %>
                                                </h5>
                                                <p class="card-text">
                                                    <%= newLesson[i].price %>円
                                                </p>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <% } %>
                        </div>
                    </div>
                </section>
                <!-- End. blog_masonry -->
            </main>
        </div>
        <!-- [id] content -->
    </div>
    <!-- End. wrapper -->

    <!-- jquery -->
    <script src="../../assets/js/jquery-3.5.0.js" type="text/javascript"></script>
    <!-- jquery-migrate -->
    <script src="../../assets/js/jquery-migrate.min.js" type="text/javascript"></script>
    <!-- popper -->
    <script src="../../assets/js/popper.min.js" type="text/javascript"></script>
    <!-- bootstrap -->
    <script src="../../assets/js/bootstrap.min.js" type="text/javascript"></script>
